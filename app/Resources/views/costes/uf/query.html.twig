{% extends "layout.html.twig" %} 
{% block titulo %} ** CONSULTA DE UNIDADES FUNCIONALES ** 
    <a href="{{path("addUf")}}" 
       class="btn btn-success">
        <span class="glyphicon glyphicon-new-window"></span> Nueva Unidad Funcional</a>
        <hr/>
{% endblock %}

{% block contenido %}
    <div class="container-fluid">
        {% for message in app.session.flashbag().get('status') %}
            <div class="alert alert-success">
                {{message}}
                <hr/>
            </div>
        {% endfor %}
        {{ sg_datatables_render(datatable) }}
    </div>
{% endblock %}